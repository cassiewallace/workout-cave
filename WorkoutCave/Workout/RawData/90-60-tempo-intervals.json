{
  "id": "90-60-tempo-intervals",
  "name": "90:60 Tempo Intervals",
  "description": "Longer tempo intervals that build muscular endurance and pacing discipline.",
  "intervals": [
    {
      "duration": 300,
      "name": "Warmup",
      "message": "Easy effort. Gradually build cadence.",
      "type": "warmup",
      "powerTarget": { "lowerBound": 0.50, "upperBound": 0.60 }
    },
    {
      "duration": 120,
      "name": "Warmup Build",
      "message": "Slightly harder to prepare for intervals.",
      "type": "steadyState",
      "powerTarget": { "lowerBound": 0.65, "upperBound": 0.75 }
    },

    {
      "duration": 90,
      "name": "Tempo",
      "message": "Strong but sustainable. No bouncing in the saddle.",
      "type": "intervalOn",
      "powerTarget": { "lowerBound": 0.80, "upperBound": 0.90 }
    },
    {
      "duration": 60,
      "name": "Recover",
      "message": "Easy spin. Let breathing settle.",
      "type": "intervalOff",
      "powerTarget": { "lowerBound": 0.45, "upperBound": 0.55 }
    },

    {
      "duration": 300,
      "name": "Cooldown",
      "message": "Smooth, easy pedaling.",
      "type": "cooldown",
      "powerTarget": { "lowerBound": 0.45, "upperBound": 0.55 }
    }
  ]
}
