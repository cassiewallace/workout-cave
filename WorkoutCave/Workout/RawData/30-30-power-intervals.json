{
  "id": "30-30-power-intervals",
  "name": "30:30 Power Intervals (80%)",
  "description": "Short, repeatable efforts focused on sustaining power and cadence. Great for both trainer and outdoor sessions.",
  "intervals": [
    {
      "duration": 300,
      "name": "Warmup",
      "message": "Build effort gradually from easy to moderately hard.",
      "type": "warmup",
      "powerTarget": { "lowerBound": 0.50, "upperBound": 0.70 }
    },

    { "duration": 30, "name": "Work", "message": "Strong, controlled effort. Hold cadence.", "type": "intervalOn",
      "powerTarget": { "lowerBound": 0.80, "upperBound": 0.85 } },
    { "duration": 30, "name": "Recover", "message": "Easy spin. Light resistance.", "type": "intervalOff",
      "powerTarget": { "lowerBound": 0.45, "upperBound": 0.55 } },

    { "duration": 30, "name": "Work", "message": "Same power as last interval.", "type": "intervalOn",
      "powerTarget": { "lowerBound": 0.80, "upperBound": 0.85 } },
    { "duration": 30, "name": "Recover", "message": "Relax shoulders. Breathe.", "type": "intervalOff",
      "powerTarget": { "lowerBound": 0.45, "upperBound": 0.55 } },

    {
      "duration": 300,
      "name": "Cooldown",
      "message": "Easy spin to bring heart rate down.",
      "type": "cooldown",
      "powerTarget": { "lowerBound": 0.45, "upperBound": 0.55 }
    }
  ]
}